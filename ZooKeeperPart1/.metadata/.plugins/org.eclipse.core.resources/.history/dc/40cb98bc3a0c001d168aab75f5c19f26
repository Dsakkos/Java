package gorillaPkg;

public class Gorilla extends Mammal {
	public Gorilla(int energyLevel) {
		super(energyLevel);
	}
	public void throwSomething() {
		System.out.println("The Gorilla Through Somthing!!" + "Energy Level: " + energyLevel);
		energyLevel -= 5;
	}
	public void eatBanana() {
		System.out.println("The Gorilla is eating banana " + "Energy Level: " + energyLevel);
		energyLevel += 10;
	}
	public void climb() {
		System.out.println("The Gorilla is climbing" + "Energy Level: " + energyLevel);
		energyLevel -= 10;
	}
}