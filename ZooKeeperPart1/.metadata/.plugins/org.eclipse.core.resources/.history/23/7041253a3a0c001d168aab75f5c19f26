package gorillaPkg;

public class Gorilla extends Mammal {
	public Gorilla(int energyLevel) {
		super(energyLevel);
	}
	public void throwSomething() {
		System.out.println("The Gorilla Through Somthing!!");
		energyLevel -= 5;
	}
	public void eatBanana() {
		System.out.println("The Gorilla is eating banana");
		energyLevel += 10;
	}
	public void climb() {
		System.out.printf("The Gorilla is climbing", energyLevel);
		energyLevel -= 10;
	}
}